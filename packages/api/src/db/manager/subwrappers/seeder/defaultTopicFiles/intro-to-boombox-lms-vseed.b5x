                    },
                    "intro-to-boombox-lms-vseed_why-boombox_f5": {
                        "uri": "intro-to-boombox-lms-vseed_why-boombox_f5",
                        "documentUri": "intro-to-boombox-lms-vseed_why-boombox",
                        "displayConditions": [
                            {
                                "type": "condition",
                                "data": {
                                    "uri": "intro-to-boombox-lms-vseed_why-boombox_f2",
