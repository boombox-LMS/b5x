                    "intro-to-boombox-lms-vseed_why-boombox_f8": {
                        "uri": "intro-to-boombox-lms-vseed_why-boombox_f8",
                        "documentUri": "intro-to-boombox-lms-vseed_why-boombox",
                        "displayConditions": [],
                        "data": {
                            "isTemplated": false
                        },
                        "contentType": "HtmlContent",
                        "isStateful": false,
                        "isRequired": false,
                        "childUris": [],
                        "contents": "<p>Think of at least two ways that the interests of the above users might clash.</p>\n",
                        "digest": "660a802d0a04fce95eb83053e84da064",
                        "dependencyUris": []
                    },
                    "intro-to-boombox-lms-vseed_why-boombox_f9": {
                        "uri": "intro-to-boombox-lms-vseed_why-boombox_f9",
                        "documentUri": "intro-to-boombox-lms-vseed_why-boombox",
                        "displayConditions": [
                            {
                                "type": "condition",
                                "data": {
                                    "uri": "intro-to-boombox-lms-vseed_why-boombox_user-clashes",
                                    "operator": "status-is",
                                    "targetValue": "completed"
                                }
                            }
                        ],
                        "data": {
                            "answers": [
                                {
                                    "id": "ae306892b3f9c1ed160fd7530634e164",
                                    "points": 1,
                                    "htmlContents": "\nTeaching team members want automatic assessments, but learners prefer realistic, open-ended challenges.\n"
                                },
                                {
                                    "id": "244106ffdb88cc12b7df0eeaaa3dddc7",
                                    "points": 1,
                                    "htmlContents": "\nLeaders want accountability, but learners want autonomy.\n"
                                },
                                {
                                    "id": "6cface426183659a4c366d6812f78b18",
                                    "points": 1,
                                    "htmlContents": "\nSubject-matter experts want to create and maintain content easily, but learners want engaging experiences.\n"
                                },
                                {
                                    "id": "54d429133242298299b3cb1f2af4c7c4",
                                    "points": 1,
                                    "htmlContents": "\nTeaching teams want the administrative features of an LMS, but subject-matter experts want to use the authoring technologies they already know.\n"
                                }
                            ]
                        },
